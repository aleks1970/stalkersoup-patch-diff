function check_spawn()
	local obj
	local lname = level.name()
	local sname = ""
	if TB3D_Modders.Global_Debug then
		TB3D_Services.packet_alert("AMK MOD: check spawn start")
	end
	if not TB3D_Modders.use_server_messages then
		get_console():execute("net_sv_log_data 0")	--user.ltx uses on/off which fails
		get_console():execute("net_cl_log_data 0")	--user.ltx uses on/off which fails
	end
	amk_anoms.init()
	spawn_unspawned_respawners() 
	if TB3D_amk_spawn then TB3D_amk_spawn.init() end
	if amk.load_variable("option_auto", 0) > 0 then					-- check levels that need lower settings to protect against ctd from bad oclusion (.HOM) files
		local dist_val = 1.5										-- most levels use max settings
		local lighting_val = 1.5									-- vision and lighting best when equal
		local lod_a = 96											-- 16 - 96
		local lod_b = 64											-- 32 - 64
		local shadow_qlty = 1.0										-- 0.5 - 1.0
		if TB3D_Modders.level_set_max < 50 then						--fixed at 30%
			dist_val = 0.7
			lighting_val = dist_val
			lod_a = 48
			lod_b = 32
			shadow_qlty = 0.5
		else
			if lname == "marsh" or lname == "swamp_old" then			--30%
				dist_val = 0.7
				lighting_val = dist_val
				lod_a = 48
				lod_b = 32
				shadow_qlty = 0.5
			elseif lname == "pripyat" or lname == "l11_Pripyat" then	--50%
				dist_val = 1.0
				lighting_val = dist_val
				lod_a = 64
				lod_b = 48
				shadow_qlty = 0.7
			elseif lname == "aver" then									--30%
				dist_val = 0.7
				lighting_val = dist_val
				lod_a = 48
				lod_b = 32
				shadow_qlty = 0.5
			elseif lname == "peshera" or lname == "av_peshera" then		--50%
				dist_val = 1.0
				lighting_val = dist_val
				lod_a = 64
				lod_b = 48
				shadow_qlty = 0.7
			elseif lname == "red_forest" or lname == "yantar_old" then	--30%
				dist_val = 0.7
				lighting_val = dist_val
				lod_a = 48
				lod_b = 32
				shadow_qlty = 0.5
			elseif TB3D_Modders.level_set_max < 70 then					--50%
				dist_val = 1.0
				lighting_val = dist_val
				lod_a = 64
				lod_b = 48
				shadow_qlty = 0.7
			elseif TB3D_Modders.level_set_max < 100 then				--70%
				dist_val = 1.2
				lighting_val = dist_val
			elseif TB3D_Modders.level_set_max > 100 then				--120%
				lighting_val = 2.0
			end
		end		--set here before renderer starts to build scene
		get_console():execute("r2_slight_fade "..utils.to_str(lighting_val))	--lighting distance (2.0 = max) but, 1099343 cought by EngineOfDarkness
		get_console():execute("rs_vis_distance "..utils.to_str(dist_val))		--matching vis dist gives best performance
		get_console():execute("r2_ssa_lod_a "..utils.to_str(lod_a))				--ssa
		get_console():execute("r2_ssa_lod_b "..utils.to_str(lod_b))				--ssa
		get_console():execute("r2_ls_squality "..utils.to_str(shadow_qlty))		--shadows
	end
	if TB3D_Modders.head_bob then
		get_console():execute("cam_inert "..utils.to_str(TB3D_Modders.head_sway_power))	--head sway and bob ON
	else
		get_console():execute("cam_inert 0")											--head sway and bob OFF
	end
	if amk.load_variable("temp_intro",0) > 0 then											--new game
		local gdiff = level.get_game_difficulty()											--starting rank
		if gdiff == 3 then
			db.actor:set_character_rank(600)
			spawn_military_btr()																--cordon south entrance	
		elseif gdiff == 2 then
			db.actor:set_character_rank(300)
			spawn_military_btr()																--cordon south entrance	
		elseif gdiff == 1 then
			db.actor:set_character_rank(100)
		else --novice
			db.actor:set_character_rank(10)
		end
