function show_news()
	if amk.load_variable("blowout",0) < 2 and amk.load_variable("option_news", 1) > 0 then
		if TB3D_Modders.use_news_message == true then TB3D_Services.packet_alert("amk mod: show news") end					--!!!!TB3D	
		local ln = level.name()
		if TB3D_Services.is_indoor(false) == false then			--no news reception indoors
			if (news_main and news_main.on_news) then news_main.on_news() end
			local rand1 = math_random(10)
			if rand1 > 5 then
				local rand2 = math_random(10)
				if rand2 < 6 then
					if rand2 == 1 then
						local ini =ini_file("misc\\TB3D_news.ltx") 
						if ini:section_exist("dolg_news") then
							news_manager.give_game_news(ini:r_string_wq("dolg_news",tostring(math.random(11))), "ui\\ui_iconsTotal", Frect():set(498,141,83,47), 0, 15000)
						end
						ini = ""
					elseif rand2 == 3 then
						local ini =ini_file("misc\\TB3D_news.ltx") 
						if ini:section_exist("freedom_news") then
							news_manager.give_game_news(ini:r_string_wq("freedom_news",tostring(math.random(6))), "ui\\ui_iconsTotal", Frect():set(498,94,83,47), 0, 15000)
						end
						ini = ""
					end
				end 
			elseif rand1 < 4 then
				local ini =ini_file("misc\\TB3D_uniq_news.ltx") 
				if ini:section_exist("news_uniq") then
					local news_item = math.random(80)
					local uniq = ini:r_string_wq("news_uniq",tostring(news_item))
					if news_item < 60 then
						local name = ini:r_string("names",tostring(math.random(300)))		--contains member callsigns
						uniq = name.."\n"..uniq
					end
