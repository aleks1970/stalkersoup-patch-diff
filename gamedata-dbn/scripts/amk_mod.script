			end
		end
	end
	if TB3D_Modders.Global_Debug then
		TB3D_Services.packet_alert("AMK MOD: check spawn 3")
	end
	TB3D_Triggers.spawn_zone_teleports()
	if (news_main) then news_main.init() end
	if TB3D_Modders.use_offline_alife then
		amk_offline_alife.init()
		amk_offline_alife.update_npc_tables()
		if TB3D_Modders.offline_alife_trade == true then amk_offline_alife.update_trade() end		--amk unique items for sale news
	end
	if TB3D_Modders.Global_Debug then
		TB3D_Services.packet_alert("AMK MOD: check spawn 4")
	end
--[[     set direction test
	obj = alife():create("vehicle_kavz685",vector():set(34.275,2.831,50.974),315023,90) -- bus in ds
	if obj then
		alife():set_switch_online(obj,false)
		alife():set_switch_offline(obj,true)
		obj.direction = vector():set(1,0,0)
		alife():set_switch_online(obj,true)
		alife():set_switch_offline(obj,false)
	end
]]--
	local blow = amk.load_variable("blowout",nil)
	-------------------------------------------------------------------------------------------
	if TB3D_Services.is_collector() == true then ---------!!!!TB3D -------------- COLLECTOR ---------
	-------------------------------------------------------------------------------------------
		if amk.load_variable("temp_intro",0) > 0 then
			if TB3D_Services.remove_info("tb3d_game_end") == true then			--achieved collector level
				local oldrep = db.actor:character_reputation()
