radiation_health_v      	= 0.0001
morale_v          			= 0.0001
health_hit_part       		= 0.9
power_hit_part        		= 0.5
psy_health_v        		= 0.01
health_restore_v            = 0.000001
sleep_health        		= 0.0
sleep_power         		= 0.0
sleep_satiety       		= 0.0
sleep_radiation       		= 0.0
sleep_psy_health      		= 0.0
