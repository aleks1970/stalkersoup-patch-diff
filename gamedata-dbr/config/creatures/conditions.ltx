;<!-- TB3D 1.0.9.9.5.0 new, excluded duplicates section aleks1970, tweaks, sleep, boar limp thrsh -->
;;;---------------------------------------------------------------------
;;; conditions are in the range of 0 thru 1, 1 being 100%
;;; satiety_v			-- rate of decrease in satiety over time
;;; radiation_v			-- the rate of reduction of radiation
;;; satiety_power_v		-- strength increases with decreasing satiety 
;;; satiety_health_v	-- increase health by reducing satiety
;;; satiety_critical	-- critical value of satiety when health begins to decrease
;;; radiation_health_v	-- reduction in health effects of radiation
;;; morale_v			-- the rate of recovery of morality
;;; health_hit_part		-- percentage of hit to reduce Health
;;; power_hit_part		-- percentage of hit causing force
;;; psy_health_v		-- recovery rate of psy-health
;;; bleeding_v			-- blood loss at nominal wound in a game second
;;; wound_incarnation_v	-- slope of the curve healing (what percentage of the wound will heal after a game second)
;;;-----------------------------------------------------------------------

[trader_condition]
satiety_v				= 1.0
radiation_v				= 1.0
satiety_power_v			= 1.0
satiety_health_v		= 1.0
satiety_critical		= 0.0
radiation_health_v		= 1.0
morale_v				= 0.0001						
health_hit_part			= 0.0
power_hit_part			= 0.0
psy_health_v			= 0.1
bleeding_v				= 0.1
wound_incarnation_v		= 1.0
min_wound_size			= 0.0
sleep_health			= 0.0
sleep_power				= 0.0
sleep_satiety			= 0.0
sleep_radiation			= 0.0
sleep_psy_health		= 0.0

[stalker_condition]
